<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainnavigation"
    app:startDestination="@id/mainFragment">


    <fragment
        android:id="@+id/catalogFragment"
        android:name="com.halal.halal_wll.mainfragment.CatalogFragment"
        android:label="fragment_catalog"
        tools:layout="@layout/fragment_catalog" />
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.halal.halal_wll.mainfragment.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_bagymdatFragment"
            app:destination="@id/bagymdatFragment" />
        <action
            android:id="@+id/action_mainFragment_to_e_CodeFragment"
            app:destination="@id/e_CodeFragment" />
    </fragment>
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.halal.halal_wll.mainfragment.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" />
    <fragment
        android:id="@+id/bagymdatFragment"
        android:name="com.halal.halal_wll.namaz_fragment.BagymdatFragment"
        android:label="fragment_bagymdat"
        tools:layout="@layout/fragment_bagymdat" />
    <fragment
        android:id="@+id/e_CodeFragment"
        android:name="com.halal.halal_wll.mainfragment.E_CodeFragment"
        android:label="fragment_e__code"
        tools:layout="@layout/fragment_e__code" />

</navigation>